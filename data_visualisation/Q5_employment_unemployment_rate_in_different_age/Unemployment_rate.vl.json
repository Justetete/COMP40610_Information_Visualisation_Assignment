{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
        "url": "https://raw.githubusercontent.com/Justetete/COMP40610_Information_Visualisation_Assignment/main/data_cleaning/quarterly_employment_unemployment_rate.csv",
        "format": {"type": "csv"}
    },
    "transform": [
        {"filter": "datum['Education Attainment Level'] == 'Levels of Education (Levels  0-8)'"},
        {"filter": "datum['Sex'] == 'Both sexes'"},
        {
            "filter": "indexof(['15 - 24 years', '25 - 54 years', '55 - 74 years'], datum['Age Group']) >= 0"
        }
    ],
    "width": 1000,
    "height": 1000,
    "title": "Unemployment rate grouped by different age",
    "mark": "line",
    "encoding": {
        "x": {
            "field": "Quarter",
            "type": "ordinal",
            "title": "Quarter"
        },
        "y": {
            "field": "Unemployment_Rate",
            "type": "quantitative",
            "title": "Rate (%)",
            "scale": {"domain": [0, 30]}
        },
        "color": {
            "field": "Age Group",
            "type": "nominal",
            "legend": {
                "orient": "top-left"},
            "scale": {
                "scheme": "category10"
            }
        }
    }
}